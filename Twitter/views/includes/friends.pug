.grid-33
 .app--section
  ul.app--user--list
   each val in twitFriends.users
    li
     .circle--fluid
      .circle--fluid--cell.circle--fluid--primary
       a.app--tweet--author
        .app--avatar(style="background-image: url('"+val.profile_image_url+"')")
         img(src=val.profile_image_url)
        h4= val.name
        p= `@${val.screen_name}`
      .circle--fluid--cell
       a.button.button-text Unfollow

